
{
    "version": 2,
    "builds": [
      {
        "src": "api/scrap.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["package.json", "package-lock.json"]
        }
      }
    ],
    "routes": [
      { "src": "/api/scrap", "dest": "api/scrap.js" }
    ],
    "env": {
      "VERCEL_ENV": "production"
    }
  }
  